    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.14.0",
    "@types/pg": "^8.11.6",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "chalk": "^5.6.2",
    "commander": "^14.0.1",
    "drizzle-kit": "^0.24.0",
    "eslint": "^8.57.0",
    "nanospinner": "^1.2.2",
    "pino-pretty": "^13.1.1",
    "prettier": "^3.3.3",
    "tsx": "^4.20.5",
    "typescript": "^5.5.4"
