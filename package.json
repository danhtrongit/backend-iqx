{
  "name": "backend-fastify",
  "version": "2.0.0",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "seed:stocks": "tsx src/scripts/seed-stock-symbols.ts",
    "seed:stocks:force": "tsx src/scripts/seed-stock-symbols.ts --force",
    "fetch:peer-comparison": "tsx src/scripts/fetch-peer-comparison.ts",
    "fetch:peer-comparison:force": "tsx src/scripts/fetch-peer-comparison.ts --force",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:start:dev": "pm2 start ecosystem.config.js --env development",
    "pm2:stop": "pm2 stop iqx-backend",
    "pm2:restart": "pm2 restart iqx-backend",
    "pm2:reload": "pm2 reload iqx-backend",
    "pm2:delete": "pm2 delete iqx-backend",
    "pm2:logs": "pm2 logs iqx-backend",
    "pm2:status": "pm2 status",
    "pm2:monit": "pm2 monit",
    "pm2:save": "pm2 save",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": ["fastify", "api", "proxy", "authentication"],
  "author": "",
  "license": "ISC",
  "description": "Professional Fastify backend with authentication and proxy",
  "dependencies": {
    "fastify": "^4.28.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/jwt": "^8.0.1",
    "@fastify/cookie": "^9.4.0",
    "@fastify/env": "^4.4.0",
    "@fastify/swagger": "^8.15.0",
    "@fastify/swagger-ui": "^4.1.0",
    "@fastify/sensible": "^5.6.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/http-proxy": "^9.5.0",
    "@fastify/reply-from": "^9.8.0",
    "fastify-plugin": "^4.5.1",
    "drizzle-orm": "^0.33.0",
    "drizzle-zod": "^0.5.1",
    "pg": "^8.12.0",
    "bcrypt": "^5.1.1",
    "zod": "^3.23.8",
    "dotenv": "^16.4.5",
    "pino": "^9.3.2",
    "pino-pretty": "^11.2.2"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@types/pg": "^8.11.6",
    "@types/bcrypt": "^5.0.2",
    "drizzle-kit": "^0.24.0",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3"
  }
}
