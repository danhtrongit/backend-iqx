{
  "name": "backend-fastify",
  "version": "2.0.0",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "seed:stocks": "tsx src/scripts/seed-stock-symbols.ts",
    "seed:stocks:force": "tsx src/scripts/seed-stock-symbols.ts --force",
    "fetch:peer-comparison": "tsx src/scripts/fetch-peer-comparison.ts",
    "fetch:peer-comparison:force": "tsx src/scripts/fetch-peer-comparison.ts --force",
    "test:analysis-reports": "tsx src/scripts/test-analysis-reports.ts",
    "fetch:analysis-reports": "tsx src/scripts/fetch-analysis-reports.ts",
    "fetch:earning-results": "tsx src/scripts/fetch-earning-results.ts",
    "fetch:earning-results:force": "tsx src/scripts/fetch-earning-results.ts --force",
    "test:earning-results": "tsx src/scripts/fetch-earning-results.ts --test VIC",
    "scraper": "tsx scripts/scraper.ts",
    "scraper:init": "tsx scripts/scraper.ts init",
    "scraper:all": "tsx scripts/scraper.ts scrape-all",
    "scraper:outdated": "tsx scripts/scraper.ts scrape-outdated",
    "scraper:jobs": "tsx scripts/scraper.ts jobs",
    "scraper:workers": "tsx scripts/scraper.ts workers",
    "scraper:queue": "tsx scripts/scraper.ts queue",
    "scraper:db-stats": "tsx scripts/scraper.ts db-stats",
    "scraper:shutdown": "tsx scripts/scraper.ts shutdown",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:start:dev": "pm2 start ecosystem.config.js --env development",
    "pm2:stop": "pm2 stop iqx-backend",
    "pm2:restart": "pm2 restart iqx-backend",
    "pm2:reload": "pm2 reload iqx-backend",
    "pm2:delete": "pm2 delete iqx-backend",
    "pm2:logs": "pm2 logs iqx-backend",
    "pm2:status": "pm2 status",
    "pm2:monit": "pm2 monit",
    "pm2:save": "pm2 save",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "fastify",
    "api",
    "proxy",
    "authentication"
  ],
  "author": "",
  "license": "ISC",
