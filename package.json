    "@fastify/helmet": "^11.1.1",
    "@fastify/http-proxy": "^9.5.0",
    "@fastify/jwt": "^8.0.1",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/reply-from": "^9.8.0",
    "@fastify/sensible": "^5.6.0",
    "@fastify/static": "^7.0.4",
    "@fastify/swagger": "^8.15.0",
    "@fastify/swagger-ui": "^4.1.0",
    "axios": "^1.12.2",
    "bcrypt": "^5.1.1",
